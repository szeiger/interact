cons Add(y, r) . x
  cut Z = y . r
  cut S(x) = Add(S(y), r) . x

cons Fib(res) . x
  cut Z = res . 1'c
  cut S(n) = Fib2(res) . n
cons Fib2(res) . x
  cut Z = res . 1'c
  cut S(n) = dup(v3, Fib(v)) . n, Fib(Add2(v, res)) . S(v3)
cons Add2(y, r) . x
  cut Z = y . r
  cut S(x) = r . S(v), x . Add2(y, v)
